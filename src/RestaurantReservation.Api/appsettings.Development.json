{
  "LogOptions": {
    "Level": "information",
    "LogTemplate": "{Timestamp:yyyy-MM-dd HH:mm:ss.fff} [{Level:u4}] {Message:lj}{NewLine}{Exception}",
    "Elastic": {
      "Enabled": true,
      "ElasticServiceUrl": "http://localhost:9200"
    },
    "File": {
      "Enabled": false,
      "Path": "logs/logs.log",
      "Interval": "day"
    }
  },
  "PostgresOptions": {
    "ConnectionString": "Server=localhost;Port=5432;Database=restaurant_reservation;User Id=postgres;Password=postgres;Include Error Detail=true"
  },
  "MongoOptions": {
    "ConnectionString": "mongodb://localhost:27017",
    "DatabaseName": "restaurant_reservations"
  },
  "HealthOptions": {
    "Enabled": false
  }
}
